insights_db:
  # the name of your JDBC driver
  driverClass: org.postgresql.Driver

  # the username
  user: ingress_user

  # the password
  password: hello ingress user

  # the JDBC URL
  url: jdbc:postgresql://localhost:5432/hello_2015_01_07

  # any properties specific to your JDBC driver:
  properties:
    charSet: UTF-8

kinesis:
  endpoint : https://kinesis.us-east-1.amazonaws.com
  streams :
    batch_pill_data : batch_pill_data

app_name: InsightsGeneratorConsumerDev

max_records: 100

# Logging settings.
logging:

  # The default level of all loggers. Can be OFF, ERROR, WARN, INFO, DEBUG, TRACE, or ALL.
  level: INFO

  # Logger-specific levels.
  loggers:

    # Sets the level for 'com.example.app' to DEBUG.
    com.hello.suripu.workers: DEBUG

insights_dynamo_db:
    endpoint : http://localhost:7777
    region: us-east-1
    table_name: insights

sleep_score_db:
    endpoint : http://localhost:7777
    region: us-east-1
    table_name: sleep_score

sleep_score_version: v_0_1

features_db:
    endpoint : http://localhost:7777
    region: us-east-1
    table_name: features

preferences_db:
  endpoint : http://localhost:7777
  region: us-east-1
  table_name: preferences

kinesis_logger:
  stream_name: dev-logs
  enabled : false
  buffer_size: 100
  origin: suripu-workers
  min_log_level : INFO